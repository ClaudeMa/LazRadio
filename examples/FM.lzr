lazradio FM-receiver;

var
  t, b, c: Integer;
  src: Rtl;
  s: Spectrum;
  fd: FreqDiscriminator;
  fm: FMReceiver;
  out: AudioOut;  

begin
  src => fd => fm => out;
  src => s;
  s :> fd;
  t := 100;
  src ! {RM_CONFIG, 3 * round(4 / 3)}
      ! {1, 2, 3} 
      ! {5, 6, 6, 8};
  write('t = ',   sin(2));
end.

