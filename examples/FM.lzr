lazradio FM-receiver;

var
  t, b, c: Integer;
  src: Rtl;
  s: Spectrum;
  fd: FreqDiscriminator;
  fm: FMReceiver;
  out: AudioOut;  

begin
  src => fd => fm => out;
  src => s;
  s :> fd;
  t := 100;
  src ! {RM_CONFIGURE, 3 * round(4 / 3), 0}
      ! {1, 2, 3} 
      ! {5,2};
  write('t = ',   sin(2));
end.

